{
	"features": [
		"simpleAppApiRoutes",
		"appRouting",
		"appConfigsTrailingSlashTrue",
		"appWasm",
		"appServerActions",
		"appGetRequestContext"
	],
	"localSetup": "npm install && npm i ../../../packages/next-on-pages",
	"buildConfig": {
		"buildCommand": "npx @cloudflare/next-on-pages",
		"buildOutputDirectory": ".vercel/output/static"
	},
	"deploymentConfig": {
		"compatibilityFlags": ["nodejs_compat"],
		"kvNamespaces": {
			"MY_KV": {
				"production": {"id": "00000000000000000000000000000000"},
				"staging": {"id": "00000000000000000000000000000000"}
			}
		}
	}
}
